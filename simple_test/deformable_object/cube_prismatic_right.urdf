<?xml version="1.0" ?>
<robot name="urdf_robot">
  <link name="baseLink">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
       <mass value="0.0"/>
       <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
      <box size="0.5 .5 .5"/>
      </geometry>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
	 <box size="0.5 .5 .5"/>
      </geometry>
    </collision>
  </link>
   <link name="childA">
    <inertial>
      <origin rpy="0 0 0" xyz="-0.5 0 0"/>
      <mass value="10.0"/>
      <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="-0.5 0 0"/>
      <geometry>
        <box size="0.5 .5 .5"/>
      </geometry>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="-0.5 0 0"/>
      <geometry>
        <box size="0.5 .5 .5"/>
      </geometry>
    </collision>
  </link>
   <joint name="joint_baseLink_childA" type="prismatic">
    <parent link="baseLink"/>
    <child link="childA"/>
    <origin xyz="0 0 0"/>
	<axis xyz="-1 0 0"/>
	<limit effort="20" lower="0.0" upper="2.0" velocity="0.2"/>
  </joint>
 
</robot>
